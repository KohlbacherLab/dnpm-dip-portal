{
    "name": "@dnpm-dip/core",
    "version": "1.0.0",
    "description": "Core Library of DNPM DIP with global constants, types & interfaces.",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        },
        "./dist/index.css": "./dist/index.css"
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist/"
    ],
    "scripts": {
        "build:types": "vue-tsc --declaration --emitDeclarationOnly -p tsconfig.build.json",
        "build:js": "rollup -c",
        "build": "rimraf ./dist && cross-env NODE_ENV=production npm run build:js && npm run build:types"
    },
    "keywords": [],
    "author": {
        "name": "Peter Placzek",
        "email": "peter.placzek1996@gmail.com",
        "url": "https://github.com/tada5hi"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/KohlbacherLab/dnpm-dip.git",
        "directory": "packages/core"
    },
    "bugs": {
        "url": "https://github.com/KohlbacherLab/dnpm-dip/issues"
    },
    "homepage": "https://github.com/KohlbacherLab/dnpm-dip#readme",
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "hapic": "^2.4.0",
        "smob": "^1.4.1",
        "ufo": "^1.3.2"
    },
    "devDependencies": {
        "bootstrap-vue-next": "^0.15.5",
        "@nuxt/schema": "^3.9.0",
        "@nuxt/kit": "^3.9.0",
        "@vuecs/link": "^1.0.0",
        "vue": "^3.4.8",
        "vue-tsc": "^1.8.27"
    },
    "peerDependencies": {
        "@nuxt/kit": "^3.9.0",
        "@vuecs/link": "^1.0.0",
        "vue": "^3.4.x"
    }
}
